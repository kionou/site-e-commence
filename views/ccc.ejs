<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
        <% if (success=='' ) { %>
            <div class="noresult">
                <p>Aucun resultat trouv√©(s) !</p>
            </div>
            <% } else if (success ){%>
                <% success.forEach(function(article){ %>
                    <div class="panier-content">
                        <div class="detail">
                            <div class="image">
                                <img src="<%= article.image %>" alt="">
                            </div>
                            <div class="nomBien">
                                <p>
                                    <%= article.nom %>
                                        <%= article.model %> -<%= article.ecran %> -<%= article.reseau %> - <%= article.rom %> -
                                                        <%= article.ram %>
                                </p>
                                <div class="supp">
                                    <button id="open" class=""><i class="fas fa-trash"></i> supprimer</button>
                                </div>
                            </div>
                        </div>
                        <div class="prix">
                            <p>
                                <%= article.prix %> F CFA
                            </p>
                            <div class="quantite">
                                <button id="decrement" onclick=""> - </button>
                                <input type="number" min="0" max="100" step="5" value="1" id="my-input" readonly>
                                <button id="increment" onclick=""> + </button>
                            </div>
                        </div>
                    </div>
                    <% }); %>
                        <% } %>
</body>
</html>






